{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)",
          "https://fonts.googleapis.com/css2?family=Barlow:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap",
          "https://fonts.googleapis.com/css?family=Poiret+One&display=swap"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "database",
      "urls": [
        "https://firebasestorage.googleapis.com/v0/b/presentup-me.appspot.com/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "7d",
        "strategy": "freshness",
        "timeout": "2s"
      }
    },
    {
      "name": "fonts",
      "urls": [
        "https://fonts.googleapis.com/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "7d",
        "strategy": "performance",
        "timeout": "2s"
      }
    }
  ]
}
