{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "database",
      "urls": [
        "https://firebasestorage.googleapis.com/v0/b/presentup-me.appspot.com/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "7d",
        "strategy": "freshness",
        "timeout": "2s"
      }
    },
    {
      "name": "fontawesome",
      "urls": [
        "https://kit-free.fontawesome.com/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "7d",
        "strategy": "performance",
        "timeout": "2s"
      }
    },
    {
      "name": "fonts",
      "urls": [
        "https://fonts.googleapis.com/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "7d",
        "strategy": "performance",
        "timeout": "2s"
      }
    }
  ]
}
